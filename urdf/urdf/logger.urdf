<?xml version="1.0" ?>



<robot name="logger" xmlns:xacro="https://www.ros.org/wiki/xacro" >
    <!-- Colors -->
    <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="blue">
    <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    <material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
    </material>
    <material name="orange">
    <color rgba="${255/255} ${108/255} ${10/255} 1.0"/>
    </material>
    <material name="brown">
    <color rgba="${222/255} ${207/255} ${195/255} 1.0"/>
    </material>
    <material name="red">
    <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <link name="chassis">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.1" radius="0.25"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.1" radius="0.25"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="2"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.032916666666666" ixy="0" ixz="0" iyy="0.032916666666666" iyz="0" izz="0.0625"/>
        </inertial>
    </link>

    <joint name="chassis2caster" type="fixed">
        <origin rpy="0 0 0" xyz="0.125 0 -0.05"/>
        <parent link="chassis"/>
        <child link="caster"/>
    </joint>

    <link name="caster">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.04"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin rpy=" 0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.04"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.000064" ixy="0" ixz="0" iyy="0.000064" iyz="0" izz="0.000064"/>
        </inertial>
        <contact>
            <lateral_friction value="0.0"/>
            <rolling_friction value="0.0"/>
            <stiffness value="30000"/>
            <damping value="1000"/>
        </contact>
    </link>

    <joint name="chassis2left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.125 -0.125 0"/>
        <parent link="chassis"/>
        <child link="left_wheel" />
        <axis rpy="0 0 0" xyz="0 1 0"/>
    </joint>

    <link name="left_wheel">
        <visual>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.09"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.04" radius="0.09"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <inertia ixx="0.0010791666666666666" ixy="0" ixz="0" iyy="0.0010791666666666666" iyz="0" izz="0.002025"/>
        <contact>
            <lateral_friction value="0.99"/>
            <rolling_friction value="0.0"/>
            <stiffness value="1000"/>
            <damping value="30000"/>
        </contact>
        </inertial>
    </link>

    <joint name="chassis2right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.125 0.125 0"/>
        <parent link="chassis"/>
        <child link="right_wheel" />
        <axis rpy="0 0 0" xyz="0 1 0"/>
    </joint>

    <link name="right_wheel">
        <collision>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.04" radius="0.09"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.09"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <inertial>
            <mass value="0.5"/>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <inertia ixx="0.0010791666666666666" ixy="0" ixz="0" iyy="0.0010791666666666666" iyz="0" izz="0.002025"/>
        </inertial>
        <contact>
            <lateral_friction value="0.99"/>
            <rolling_friction value="0.0"/>
            <stiffness value="1000"/>
            <damping value="30000"/>
        </contact>
    </link>


</robot>
